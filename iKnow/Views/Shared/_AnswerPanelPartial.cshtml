@model iKnow.Models.Answer

@Html.Partial("_AnswerPanelHeaderPartial")
<div class="answer-panel-content">
    <div class="answer-panel-content-inner">@Html.Raw(Model.Content)</div>
    <div class="answer-footer">
        <span>Answered @Model.CreatedDate.ToString("MMM d, yyyy")</span>
    </div>
    @Html.Action("EditIcon", "Answer", new { id = Model.Id })
</div>
